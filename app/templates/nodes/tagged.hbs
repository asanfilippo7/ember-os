<h3>All Tagged Nodes</h3>
{{#link-to 'nodes'}}All nodes{{/link-to}}
<ul>
{{#each model as |node|}}
    <li>
       
        {{#link-to 'nodes.node' node}}<h3>{{node.title}}</h3>{{/link-to}}
        <p>{{node.dateCreated}}</p>
           <ul>
                {{#each node.tags as |tag|}}
                    <li>{{tag}}</li>
                {{/each}}
            </ul>
        <br>
    </li>  
{{/each}}
</ul>

